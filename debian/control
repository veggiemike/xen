Depends: bash, __deplibs__
Replaces: xen-hypervisor, xen-system, xen-utils, xen-utils-common, libxenmisc4.11, libxendevicemodel1, libxencall1, libxenevtchn1, libxenforeignmemory1, libxengnttab1, xenstore-utils, libxenstore3.0, libxentoolcore1, libxentoollog1, qemu-system-x86-xen
